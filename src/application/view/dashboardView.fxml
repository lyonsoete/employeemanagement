<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.DashboardController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <children>
    <Pane prefHeight="200.0" prefWidth="200.0">
      <children>
        <Label layoutX="221.0" layoutY="14.0" text="Dashboard Hardwareverwaltung">
          <font>
            <Font name="System Bold" size="24.0" />
          </font>
        </Label>
        <Button fx:id="logoutBtn" layoutX="669.0" layoutY="17.0" mnemonicParsing="false" onAction="#logout" prefHeight="30.0" prefWidth="107.0" text="Logout">
          <font>
            <Font size="14.0" />
          </font>
        </Button>
        <Button fx:id="realEstateBtn" layoutX="230.0" layoutY="54.0" mnemonicParsing="false" onAction="#realEstate" prefHeight="30.0" prefWidth="350.0" text="Immobilienverwaltung">
          <font>
            <Font size="14.0" />
          </font>
        </Button>
        <Button fx:id="tenantBtn" layoutX="230.0" layoutY="84.0" mnemonicParsing="false" onAction="#tenant" prefHeight="30.0" prefWidth="350.0" text="Mieterverwaltung">
          <font>
            <Font size="14.0" />
          </font>
        </Button>
        <Button fx:id="additionalCostsBtn" layoutX="230.0" layoutY="114.0" mnemonicParsing="false" onAction="#additionalCosts" prefHeight="30.0" prefWidth="350.0" text="Nebenkosten, Zahlungen und Außenstände">
          <font>
            <Font size="14.0"/>
          </font>
        </Button>
        <Button fx:id="documentsBtn" layoutX="230.0" layoutY="144.0" mnemonicParsing="false" onAction="#documents" prefHeight="30.0" prefWidth="350.0" text="Dokunmentenmanagement">
          <font>
            <Font size="14.0"/>
          </font>
        </Button>
        <Button fx:id="usersBtn" layoutX="230.0" layoutY="174.0" mnemonicParsing="false" onAction="#users" prefHeight="30.0" prefWidth="350.0" text="Benutzerverwaltung">
          <font>
            <Font size="14.0"/>
          </font>
        </Button>
<!--        <Button fx:id="druckerBtn" layoutX="230.0" layoutY="204.0" mnemonicParsing="false" onAction="#druckerverwaltung" prefHeight="30.0" prefWidth="350.0" text="Report erstellen">-->
<!--          <font>-->
<!--            <Font size="14.0"/>-->
<!--          </font>-->
<!--        </Button>-->
      </children>
    </Pane>
  </children>
</GridPane>
