<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.RealEstateController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <children>
    <Pane prefHeight="200.0" prefWidth="200.0">
      <children>
        <Label layoutX="272.0" layoutY="14.0" text="Immobilien Verwaltung">
          <font>
            <Font name="System Bold" size="24.0" />
          </font>
        </Label>
        <Button fx:id="dashboardBtn" layoutX="620.0" layoutY="540.0" mnemonicParsing="false" onAction="#dashboard" prefHeight="30.0" prefWidth="150.0" text="Dashboard">
          <font>
            <Font size="14.0" />
          </font>
        </Button>
        <Button fx:id="newRealEstate" layoutX="20.0" layoutY="340.0" mnemonicParsing="false" onAction="#createRealEstate" prefHeight="30.0" prefWidth="150.0" text="neue Immobilie">
          <font>
            <Font size="14.0" />
          </font>
        </Button>
      </children>
    </Pane>
  </children>
</GridPane>
